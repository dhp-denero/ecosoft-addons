<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <act_window name="View Product in Stock"
	        res_model="stock.location.product"
	        src_model="sale.order"
	        view_mode="form"
	        target="new"
	        context="{'product_field':['order_line', 'product_id']}"
	        key2="client_action_multi"
	        id="stock_product_act_window"
	        view_id="view_stock_check_availability"/>
        
        <act_window name="View Part in Stock"
	        res_model="stock.location.product"
	        src_model="sale.order"
	        view_mode="form"
	        target="new"
	        context="{'product_field':['child_ids', 'product_lines', 'product_id']}"
	        key2="client_action_multi"
	        id="stock_product_act_window"
	        view_id="view_stock_check_availability"/>        
	</data>
</openerp>