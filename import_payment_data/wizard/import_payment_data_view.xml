<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_import_payment_data" model="ir.ui.view">
            <field name="name">view.import.payment.data</field>
            <field name="model">import.payment.data</field>
            <field name="arch" type="xml">
                <form string="Credit Note" version="7.0">
                    <group>
                         <group>
                             <field name="name" on_change="onchange_name(name)" />
                         </group>
                         <group>
                         </group>
                    </group>
                    <footer>
                        <button string='Import Data' name="import_data" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
               </form>
            </field>
        </record>

        <record id="action_import_data" model="ir.actions.act_window">
            <field name="name">Import Payment Data</field>
            <field name="res_model">import.payment.data</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
           	<field name="view_id" ref="view_import_payment_data"/>
           	<field name="target">new</field>
        </record>


    </data>
</openerp>
